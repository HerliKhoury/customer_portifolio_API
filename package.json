  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.11.1",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.17",
    "zod": "^3.21.4"
  },
  "devDependencies": {
